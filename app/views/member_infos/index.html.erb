<p id="notice"><%= notice %></p>

<h1>Listing Member Infos</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone number</th>
      <th>Address</th>
      <th>Email</th>
      <th>Gender</th>
      <th>Age</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @member_infos.each do |member_info| %>
      <tr>
        <td><%= member_info.first_name %></td>
        <td><%= member_info.last_name %></td>
        <td><%= member_info.phone_number %></td>
        <td><%= member_info.address %></td>
        <td><%= member_info.email %></td>
        <td><%= member_info.gender %></td>
        <td><%= member_info.age %></td>
        <td><%= member_info.latitude %></td>
        <td><%= member_info.longitude %></td>
        <td><%= link_to 'Show', member_info %></td>
        <td><%= link_to 'Edit', edit_member_info_path(member_info) %></td>
        <td><%= link_to 'Destroy', member_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member info', new_member_info_path %>
